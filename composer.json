{
    "name": "lelectrolux/content-links",
    "description": "Search for links in your models content, and check them for errors",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "laravel/framework": "^12.0",
        "maatwebsite/excel": "^3.1.0",
        "guzzlehttp/guzzle": "^7.9.0",
        "nesbot/carbon": "^3.10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.22.0",
        "laravel/tinker": "^2.10.0"
    },
    "autoload": {
        "psr-4": {
            "Lelectrolux\\ContentLinks\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lelectrolux\\ContentLinks\\ContentLinksServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Lelectrolux",
            "email": "Lelectrolux@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "pint": [
            "@php vendor/bin/pint --ansi"
        ]
    }
}
